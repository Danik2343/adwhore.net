{
  "name": "__MSG_extension_name__",
  "version": "0.0.2.2",
  "version_name": "alpha",
  "description": "__MSG_extension_description__",
  "manifest_version": 2,
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "*://www.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "adwhore.js",
        "jquery.min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "img/adwhore-logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab", "storage", "https://karma.adwhore.net/*", "https://sponsor.ajay.app/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "img/*"
  ]
}
